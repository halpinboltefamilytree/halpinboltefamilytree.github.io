      options.pageFitMode = primitives.PageFitMode.AutoSize;
      options.items = items;
      options.cursorItem = 50;
      options.linesWidth = 3;
      options.linesColor = "black";
      options.hasSelectorCheckbox = primitives.Enabled.False;
      options.normalLevelShift = 50;
      options.lineLevelShift = 50;
      options.normalItemsInterval = 50;
      options.lineItemsInterval = 50;
      options.defaultTemplateName = "tree";
      options.templates = [getTreeTemplate()];
      options.onItemRender = onTemplateRender;
      options.bevelSize = 1;
      options.elbowDotSize = 1;

      control = primitives.FamDiagram(document.getElementById("basicdiagram"), options);
    });

    function onTemplateRender(event, data) {
      switch (data.renderingMode) {
        case primitives.RenderingMode.Create:
          /* Initialize template content here */
          break;
        case primitives.RenderingMode.Update:
          /* Update template content here */
          break;
      }

      var itemConfig = data.context;

      if (data.templateName == "tree") {
        var title = data.element.firstChild;
        title.textContent = itemConfig.title;
      }
    }

    function getTreeTemplate() {
      var result = new primitives.TemplateConfig();
      result.name = "tree";

      result.itemSize = new primitives.Size(175, 60);


      result.itemTemplate =
        '<div class="tree-item tree-corner-all tree-item-frame" style="width:' + result.itemSize.width + 'px; height: ' + result.itemSize.height + 'px;">'
        + '<div name="title" class="tree-item">'
        + '</div>'
        + '</div>';

      return result;
    }
